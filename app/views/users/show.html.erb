<h1><%= @user.username %></h1>
<h3>User created on <%= @user.created_at %></h3>
<h3>Last Signed in on <%= @user.last_sign_in_at %></h3>
<% @items.each do |i|%>
    <%= render 'items/item', item: i %>
<% end %>
<% if user_signed_in? %>
    <% if @user == current_user%>
        <%= render 'items/form', item: Item.new %>
    <% end %>
<% end %>